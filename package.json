{
  "name": "bsc-web-interface",
  "version": "1.0.0",
  "description": "BSC Web Interface with ML-powered YouTube video analysis",
  "main": "backend/server.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon backend/server.js",
    "start": "node backend/server.js",
    "install-all": "npm install && cd frontend && npm install",
    "setup": "npm run install-all && cd ml-service && python setup.py",
    "frontend": "cd frontend && npm run dev",
    "ml-service": "cd ml-service && python app.py"
  },
  "keywords": ["youtube", "emotion-analysis", "ml", "web-interface"],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.10.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "mongoose": "^8.12.1"
  },
  "devDependencies": {
    "nodemon": "^3.1.7"
  }
}
